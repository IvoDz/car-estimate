   "source": [
    "from scipy.stats import f\n",
    "\n",
    "# Given values\n",
    "alpha = 0.01\n",
    "df1 = 3\n",
    "df2 = 20\n",
    "\n",
    "# Calculate critical F-value\n",
    "critical_f_value = f.ppf(1 - alpha, df1, df2)\n",
    "\n",
    "print(\"Critical F-value:\", critical_f_value)"
   ]
